<?xml version="1.0" encoding="UTF-8"?>
<!--映射文件配置 -->
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- namespace命名空间，作用：对sql进行分类化管理 -->
<mapper namespace="com.kano.dao.ICourseDao">
    <select id="selectId" resultType="com.kano.domain.Course">
        select *
        from course
        where course_no=#{course_no}
    </select>
</mapper>